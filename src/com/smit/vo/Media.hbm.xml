<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class name="com.smit.vo.Media" table="media">
 <id name="id">
 <generator class="native"></generator>
 </id>

 <property name="fileName" column="filename" />
 <property name="path"/>
  <property name="createtime" />
 <property name="source"/>
 <property name="fileSize" column="filesize"></property>
 <property name="typeName" column="typename"></property>
 <property name="userName" column="username"></property>
   <property name="sortRank" column="sortrank"/>
 <!--<many-to-one name="mediaType" class="com.smit.vo.MediaType" column="tid"></many-to-one>-->  

</class>
</hibernate-mapping>
