<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.smit.vo.AtomRecord" table="atom_record">
		<id name="id">
			<generator class="native"></generator>
		</id>
		<property name="title"></property>
		<property name="link"></property>
		<property name="atom_id"></property>
		<property name="update_time"></property>
		<property name="feed_url"></property>
		<property name="create_time"></property>
		
	</class>
</hibernate-mapping>

