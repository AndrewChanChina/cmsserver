<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="com.smit.vo.Video" table="video">
  <id name="id">
   <generator class="native"/>
  </id>
  <property generated="never" lazy="false" name="title"/>
  <property generated="never" lazy="false" name="description" type="text"/>
  <property generated="never" lazy="false" name="link"/>
  <property generated="never" lazy="false" name="author"/>
  <property generated="never" lazy="false" name="guid"/>
  <property generated="never" lazy="false" name="size"/>
  <property generated="never" lazy="false" name="comments"/>
  <property generated="never" lazy="false" name="pubDate"/>
  <property generated="never" lazy="false" name="enclosure_url"/>
  <property generated="never" lazy="false" name="flv_url"/>
  <property generated="never" lazy="false" name="flag1"/>
  <property generated="never" lazy="false" name="flag2"/>
  <property generated="never" lazy="false" name="createtime"/>
  <property column="part_id" generated="never" lazy="false" name="partId"/>
  <property name="img"></property>
 </class>
</hibernate-mapping>
