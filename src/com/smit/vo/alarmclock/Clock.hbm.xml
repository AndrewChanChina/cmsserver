<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.smit.vo.alarmclock.Clock" table="clock">
		<id name="id">
			<generator class="native"></generator>
		</id>
		<property name="name"></property>
		<property name="hour"></property>
		<property name="minutes"></property>
		<property name="dayofweek"></property>
		<property name="alarmtime"></property>
		<property name="enable"></property>
		<property name="vibrate"></property>
		<property name="message"></property>
		<property name="alert"></property>
		<property name="music"></property>
		<property name="status"></property>
		<property name="userid"></property>
		
	</class>
</hibernate-mapping>

