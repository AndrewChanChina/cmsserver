<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class name="com.smit.vo.MediaType" table="mediatype">
 <id name="id">
 <generator class="native"></generator>
 </id>
 <property name="typeName" column="typename" />
 <property name="isspecial"/>
  <property name="topid" />
 <property name="father_id"/>
 <set name="medias" table="media" lazy="true" inverse="true" cascade="all">
  <key column="tid"></key>
  <one-to-many class="com.smit.vo.Media" />
 </set>
</class>
</hibernate-mapping>
