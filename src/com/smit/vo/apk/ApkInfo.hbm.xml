<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.smit.vo.apk.ApkInfo" table="apk_info">
		<id name="id">
			<generator class="native"></generator>
		</id>
		<property name="status"></property>
		<property name="roomnum"></property>
		<property name="iconUrl"></property>
		<property name="appName"></property>
		<property name="packageName"></property>
		<property name="operation"></property>
		<property name="apkUrl"></property>
		<property name="type"></property>
		
	</class>
</hibernate-mapping>

