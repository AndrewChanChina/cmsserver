<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.smit.vo.TestOption" table="test_option" >
		<id name="id">
			<generator class="native"></generator>
		</id>
		<property name="name"></property>
		<property name="test_id"></property>
		<property name="create_time"></property>
		 
		<set name="detailLogs" cascade="all" lazy="false" inverse="true">
			<key column="testOption_id"></key>
			<one-to-many class="com.smit.vo.DetailLog"/>
		</set>
		<set name="orders" table="tb_order_option"  inverse="true" lazy="false" cascade="save-update,delete">
			<key column="option_id"></key>
			<many-to-many class="com.smit.vo.Order" column="order_id" ></many-to-many>
		</set>
	</class>
</hibernate-mapping>

